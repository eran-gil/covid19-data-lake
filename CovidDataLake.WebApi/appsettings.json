{
  "Kafka": {
    "Instances": [
      {
        "Host": "localhost",
        "Port": 9092
      }
    ],
    "Topic": "covid-data-lake"
  },
  "Validation": {
    "AllowedFileTypes": [ ".csv", ".json" ]
  },
  "RedisIndexCache": {
    "LockDurationInSeconds": 10
  },
  "AmazonIndexFile": {
    "BucketName": "bucket"
  },
  "AmazonRootIndex": {
    "BucketName": "bucket",
    "RootIndexName": "root_index.txt",
    "LockTimeSpanInSeconds": 30
  },
  "Redis": "localhost:6379",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
