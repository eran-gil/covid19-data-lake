{
  "Kafka": {
    "Instances": [
      {
        "Host": "localhost",
        "Port": 9092
      }
    ],
    "Topic":  "covid-data-lake" 
  },
  "Storage": {
    "BasePath":  "E:\\DataLake" 
  },
