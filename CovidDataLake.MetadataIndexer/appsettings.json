{
  "Kafka": {
    "Instances": [
      {
        "Host": "localhost",
        "Port": 9092
      }
    ],
    "GroupId": "MetadataIndexer",
    "Topic": "covid-data-lake"
  },
  "MetadataExtraction": {
    "IgnoreMetadataKeys": [ "X-Parsed-By", "FilePath" ]
  },
  //HyperLogLog__BucketName
  //HyperLogLog__BucketName
  //AmazonIndexFile__BucketName
  "HyperLogLog": {
    "BucketName": "bucket",
    "LockIntervalInSeconds": 30
  },
  "CountMinSketch": {
    "BucketName": "bucket",
    "LockIntervalInSeconds": 30,
    "ErrorRate": 0.1,
    "Confidence": 0.9
  },
  "AmazonIndexFile": {
    "BucketName": "bucket"
  },
  "AmazonGeneralConfig": {
    "ServiceUrl": "aws_service_url"
  },
  "BatchConfig": {
    "BatchIntervalInSeconds": 5
  },
  "Redis": "localhost:6379"
}